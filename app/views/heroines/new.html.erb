<h1>Create a new Superheroine!</h1>

<%= form_for @heroine do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>
  <%= f.label :super_name %>
  <%= f.text_field :super_name %><br><br>
  <%= f.label :power %>
  <%= f.collection_select(:power_id, Power.all, :id, :name, prompt: true) %><br><br>
  <%= f.submit %>
<% end %>
