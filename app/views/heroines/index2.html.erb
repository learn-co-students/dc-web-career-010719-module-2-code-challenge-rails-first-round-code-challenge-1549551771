<%= link_to "Create a super hero!", new_heroine_path %>
<h2>All Heroines</h2>
<%= form_tag("/search", method: "get") do %>
  <%= label_tag(:q, "Search for a power:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>
<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>
